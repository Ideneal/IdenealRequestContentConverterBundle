{
    "name": "ideneal/request-content-converter-bundle",
    "description": "This bundle provides a way to convert and validate the request content specifying a format",
    "keywords": [
        "request",
        "content",
        "deserialization",
        "format"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name": "Daniele Pedone",
        "email": "ideneal.ztl@gmail.com"
    }],
    "require": {
        "php": ">=7.1.3",
        "symfony/config": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "doctrine/annotations": "^1.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/serializer-pack": "dev-master",
        "symfony/validator": "^5.1@dev",
        "symfony/orm-pack": "dev-master",
        "symfony/expression-language": "^5.1@dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3.5|^5.0",
        "phpunit/phpunit": "^7"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Ideneal\\Bundle\\RequestContentConverterBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ideneal\\Bundle\\RequestContentConverterBundle\\Tests\\": "tests/",
            "Tests\\Fixtures\\": "tests/Fixtures/"
        }
    }
}
